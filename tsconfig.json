{
    "version": "1.5.0",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "outDir": "dist"
    },
    "filesGlob": [
        "typings/tsd.d.ts",
        "src/**/*.ts"
    ],
    "files": [
        "typings/tsd.d.ts",
        "src/app/bootstrap.ts",
        "src/app/components/app.ts",
        "src/app/components/dashboard/dashboard.ts",
        "src/app/components/home/home.ts",
        "src/app/components/todo/todo.ts",
        "src/app/directives/Autofocus.ts",
        "src/app/directives/directives.ts",
        "src/app/services/TodoService.ts",
        "src/app/services/services.ts"
    ]
}
